    "bcrypt": "^5.1.1",
    "bullmq": "^5.8.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.0",
    "express-handlebars": "^7.1.3",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "nodemailer": "^6.9.14",
    "nodemon": "^3.1.4",
    "prisma": "^5.16.2",
    "socket.io": "^4.7.5",
